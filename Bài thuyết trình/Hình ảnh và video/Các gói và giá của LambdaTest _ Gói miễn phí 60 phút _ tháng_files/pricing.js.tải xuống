  $scope.display_code3  = $scope.items[3].products[0][$scope.interval].code;
  $scope.changePlan = function(durationType) {
    $scope.interval = durationType;
    if(durationType === "year"){
      $scope.planIndex = 0;
    }else{
      $scope.planIndex = 1;
    }
    $scope.row = $scope.items[1].products[0].name;
    $scope.row2 = $scope.items[2].products[0].name;
    $scope.row3 = $scope.items[3].products[0].name;
