    $scope.display_code  = $scope.items[1].products[0][$scope.interval].code;
    $scope.display_code2  = $scope.items[2].products[0][$scope.interval].code
  }
  $scope.onChange = function(item, $index){
    $scope.priceCnange = item; 
  }

  $scope.onloadFun = function($index) {
    if ($index ==1) {
      $scope.selectRow = function(item, i){
       $scope.row = $scope.items[$index].products[i].name;
       $scope.display_price = item[$scope.interval].display_price;
       $scope.display_code  = item[$scope.interval].code
