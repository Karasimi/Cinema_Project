  $scope.items = $scope.itemsBig.data;

  if($scope.items && $scope.items.length){
    $scope.items.forEach( function(el) {
      if(el.products && el.products.length){
        el.products.forEach(function(product) {
          if(product.plans && product.plans.length){
            product.plans.forEach(function(plan) {
              if(plan && plan.interval === "month"){
                product.month = plan
              }else if(plan && plan.interval === "year"){
                product.year = plan
              }
            });
          }
        });
      }
    });
  }

  $scope.loader= false;
  $scope.row = $scope.items[1].products[0].name;
  $scope.row2 = $scope.items[2].products[0].name;
  $scope.row3 = $scope.items[3].products[0].name;
  $scope.display_price = $scope.items[1].products[0][$scope.interval].display_price;
  $scope.display_price2 = $scope.items[2].products[0][$scope.interval].display_price;
  $scope.display_price3 = $scope.items[3].products[0][$scope.interval].display_price;
